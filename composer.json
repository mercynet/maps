{
  "name": "mercynet/maps",
  "description": "A package to load maps with support for Leaflet and Google Maps",
  "version": "1.0.0",
  "license": "MIT",
  "require": {
    "php": "^8.3"
  },
  "autoload": {
    "psr-4": {
      "Maps\\": "src/"
    }
  },
  "require-dev": {
    "pestphp/pest": "^3.2"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
