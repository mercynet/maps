{
  "name": "mercynet/maps",
  "description": "A package to load maps with support for Leaflet and Google Maps",
  "version": "1.0.2",
  "license": "MIT",
  "require": {
    "php": "^8.3",
    "google/apiclient": "^2.17"
  },
  "autoload": {
    "psr-4": {
      "Maps\\": "src/"
    }
  },
  "require-dev": {
    "pestphp/pest": "^3.2",
    "phpstan/phpstan": "^1.12"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
